#include <stdio.h>

struct Node {
    int data;
    struct Node *left;
    struct Node *right;
};


void inorder(struct Node *root) {
    if (root != 0) {
        inorder(root->left);
        printf("%d ", root->data);
        inorder(root->right);
    }
}


void postorder(struct Node *root) {
    if (root != 0) {
        postorder(root->left);
        postorder(root->right);
        printf("%d ", root->data);
    }
}

int main() {
    
    struct Node n1 = {3, 0, 0};
    struct Node n2 = {9, 0, 0};
    struct Node n3 = {20, 0, 0};
    struct Node n4 = {15, 0, 0};
    struct Node n5 = {7, 0, 0};

    
    n1.left = &n2;
    n1.right = &n3;
    n3.left = &n4;
    n3.right = &n5;

    printf("Inorder Traversal: ");
    inorder(&n1);

    printf("\nPostorder Traversal: ");
    postorder(&n1);

    return 0;
}
